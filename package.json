{
  "scripts": {
    "serve": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "concurrently \"yarn start:server\" \"yarn start:client\"",
    "start:server": "nodemon ./src/index.ts",
    "start:client": "cd client && yarn dev",
    "build": "yarn build:server && yarn build:client",
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "cd client && yarn build",
    "db:migrate:main": "npx prisma migrate dev --schema './src/databases/maindb/schema.prisma'",
    "db:migrate:main:prod": "npx prisma migrate dev --schema './src/databases/maindb/schema.prisma'",
    "db:reset": "npx prisma migrate reset --force --schema './src/databases/maindb/schema.prisma'",
    "cleanDev": "yarn db:reset && ts-node ./src/seed && yarn dev",
    "codegen": "concurrently \"yarn start:server\" \"cd client && yarn codegen:schema\""
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/register": "^7.18.9",
    "@innovixx/eslint-config": "^1.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/node": "^16.18.11",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "concurrently": "^7.6.0",
    "eslint": ">= 7.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.9.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@apollo/server": "^4.3.2",
    "@graphql-tools/utils": "^9.1.4",
    "@prisma/client": "^4.9.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "helmet": "^6.0.1",
    "lodash.merge": "^4.6.2"
  }
}
